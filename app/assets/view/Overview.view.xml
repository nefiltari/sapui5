<mvc:View
    controllerName="sap.ui.pt.hana.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
  <Page title="{i18n>homePageTitle}">
    <headerContent>
      <Button
          icon="sap-icon://hello-world"
          press="onOpenDialog"/>
    </headerContent>
    <content>
      <mvc:XMLView viewName="sap.ui.pt.hana.view.HelloPanel"/>
      <mvc:XMLView viewName="sap.ui.pt.hana.view.InvoiceList"/>
    </content>
  </Page>
</mvc:View>